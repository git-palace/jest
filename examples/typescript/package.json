{
  "dependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "jest-cli": "*",
    "react-addons-test-utils": "*",
    "typescript": ">=1.6.2"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/*"
    ]
  }
}
